                            <div class="prt-overlay">
                                <span class="prt-icon zoom-icon" style="--i: 0s">
                                    <i class="uil uil-search-plus"></i>
                                </span>
                                <a href="#" class="prt-icon" style="--i: .15s">
                                    <i class="uil uil-link-h"></i>
                                </a>
                            </div>
                        </div>
                        <div class="prt-desc">
                            <h3>Gestor de Matriculas</h3>
                            <a href="*" class="btn secondary-btn sm">Leer mas</a>

                        </div>
                    </div>
                </div>
            </div>
        </section>


    </main>
    <!-- --------- Footer --------- -->
    <%- include('templates/footer'); %>

    <!-- JavaScript Files -->

    <script src="https://unpkg.com/swiper@8/swiper-bundle.min.js"></script>
    <script src="js/mixitup.min.js"></script>
    <script src="js/app.js"></script>
</body>

</html>